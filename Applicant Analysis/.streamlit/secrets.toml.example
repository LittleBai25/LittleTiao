# API keys for various LLM providers
# Copy this file to .streamlit/secrets.toml and add your actual API keys

# Qwen API (required for transcript analysis and can be used for other agents)
QWEN_API_KEY = "your_qwen_api_key_here"

# OpenAI API (optional, for GPT models)
OPENAI_API_KEY = "your_openai_api_key_here"

# Anthropic API (optional, for Claude models)
ANTHROPIC_API_KEY = "your_anthropic_api_key_here"

# DeepSeek API (optional, for DeepSeek models)
DEEPSEEK_API_KEY = "your_deepseek_api_key_here"

# Serper Web Search API (for program recommendations)
SERPER_API_KEY = "your_serper_api_key_here"
SMITHERY_API_KEY = "your_smithery_api_key_here" 